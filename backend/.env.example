# These are set via Modal secrets in production.
# For local development, copy this to .env and fill in values.

# LLM
GROQ_API_KEY=your-groq-api-key

# Firebase
FIREBASE_SERVICE_ACCOUNT={"type":"service_account","project_id":"..."}

# Redis
REDIS_HOST=your-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# Search & Maps
YELP_API_KEY=your-yelp-api-key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
USDA_API_KEY=your-usda-api-key

# Delivery
DOORDASH_DEVELOPER_ID=your-doordash-developer-id
DOORDASH_KEY_ID=your-doordash-key-id
DOORDASH_SIGNING_SECRET=your-doordash-signing-secret

# Payments
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# Voice (VAPI)
VAPI_API_KEY=your-vapi-api-key

# Web Automation (TinyFish)
TINYFISH_API_KEY=your-tinyfish-api-key

# Authenticated Browsing (platform credentials)
PARTYSLATE_EMAIL=your-partyslate-email
PARTYSLATE_PASSWORD=your-partyslate-password

# Slack
SLACK_BOT_TOKEN=your-slack-bot-token
SLACK_SIGNING_SECRET=your-slack-signing-secret

# Email (Brevo)
BREVO_API_KEY=your-brevo-api-key

# Grocery (Instacart)
INSTACART_API_KEY=your-instacart-api-key

# Uber Direct
UBER_DIRECT_CLIENT_ID=your-uber-client-id
UBER_DIRECT_CLIENT_SECRET=your-uber-client-secret

# Unsplash
UNSPLASH_ACCESS_KEY=your-unsplash-access-key
